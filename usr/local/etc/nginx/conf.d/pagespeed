pagespeed on;
pagespeed FileCachePath /var/local/cache/psi;
pagespeed FileCacheSizeKb            102400;
pagespeed FileCacheCleanIntervalMs   3600000;
pagespeed FileCacheInodeLimit        500000;
#pagespeed EnableFilters rewrite_css,rewrite_javascript,combine_css,combine_javascript,inline_javascript,prioritize_critical_css,inline_preview_images;
pagespeed MemcachedTimeoutUs 100000;
pagespeed Domain maxcdn.bootstrapcdn.com;
pagespeed Domain ajax.googleapis.com;
pagespeed Domain alphageek.xyz;

location ~ "\.pagespeed\.([a-z]\.)?[a-z]{2}\.[^.]{10}\.[^.]+" {
    add_header "" "";
}

location ~ "^/pagespeed_static/" { }
location ~ "^/ngx_pagespeed_beacon$" { }

